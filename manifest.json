{
  "background": {
    "scripts": ["bower_components/angular/angular.js", "bower_components/angularjs-indexedDB/src/indexeddb.js", "js/tabmanager.js", "eventPage.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "name": "Tab Wrangler",
  "options_page":"popup.html",
  "version": "3.1.0",
  "description": "Automatically closes inactive tabs and makes it easy to get them back", 
  "minimum_chrome_version": "28",
  "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png",
      "16": "img/icon16.png"
   },
  "browser_action": {
    "default_title": "Tab Wrangler",
    "default_icon": "img/icon.png",
    "default_popup":"popup.html"
  },
  "web_accessible_resources": [
    "img/icon48.png"
  ],
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "contextMenus",
    "alarms"
  ]
}
